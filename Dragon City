end
-----Watch ADS

function Function4()
gg.timeJump('14:00')
end
-----Time Jump

function Function6()
gg.setRanges(gg.REGION_C_ALLOC)
gg.searchNumber("Q'tid_'", gg.TYPE_BYTE, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.processResume()
revert = gg.getResults(200000, nil, nil, nil, nil, nil, nil, nil, nil)
gg.editAll("Q ''", gg.TYPE_BYTE)
gg.clearResults()
end

function Funciton7()
gg.setRanges(gg.REGION_C_ALLOC | gg.REGION_C_DATA)
gg.searchNumber("Q 'expansions'", gg.TYPE_BYTE, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.processResume()
revert = gg.getResults(5000, nil, nil, nil, nil, nil, nil, nil, nil)
gg.editAll("Q 'h'", gg.TYPE_BYTE)
gg.clearResults()
end

function Function8()
Move = gg.multiChoice({
'Move 6 Free',
'Move 5 Free',
'Move 4 Free,',
'Move 3 Free',
'Move 2 Free',
'Move 1 Free',
'Back',
}
,nil,
'Move Habitat Free')

if Move == nil then else
if Move[1] == true then Move1() end
if Move[2] == true then Move2() end
if Move[3] == true then Move3() end
if Move[4] == true then Move4() end
if Move[5] == true then Move5() end
if Move[6] == true then Move6() end
if Move[7] == true then Start() end end end

function Move1()
gg.setRanges(gg.REGION_C_ALLOC)
gg.searchNumber("1086324736;1086324736::5", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
revert = gg.getResults(5000, nil, nil, nil, nil, nil, nil, nil, nil)
gg.editAll("0", gg.TYPE_DWORD)
gg.clearResults()
end

