function Noselect()
gg.sleep(1)
end


print('Thanks you because using we script !!! Happy a day')

while(true) do
if gg.isVisible(true) then
MainMenu = 1
MainMenu2 = 2
gg.setVisible(false)
end

if MainMenu == 1 then Start() end
end
