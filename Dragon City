function Move6()
  gg.setRanges(gg.REGION_C_ALLOC)
   gg.searchNumber("-300~300F;1 065 353 216;1 065 353 216;2~1000;10~10000::17", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
   gg.refineNumber("1 065 353 216", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
   gg.getResults(5000)
   gg.editAll("0", gg.TYPE_DWORD)
   gg.clearResults()
end


function Function9()
Habitat = gg.multiChoice({'Nature','Ice','Electric','Metal','Dark','Legend','Pure','Light','War','Primal','Wind','Back to Menu'},nil,'Skip Time Habitat [ Level 1 ]')

if Habitat[1] == true then Nature() end
if Habitat[2] == true then Ice() end
if Habitat[3] == true then Electric() end
if Habitat[4] == true then Metal() end
if Habitat[5] == true then Dark() end
if Habitat[6] == true then Legend() end
if Habitat[7] == true then Pure() end
if Habitat[8] == true then Light() end
if Habitat[9] == true then War() end
if Habitat[10] == true then Primal() end
if Habitat[11] == true then Wind() end
if Habitat [12] == true then Start() end
if Habitat == nil then else
end
end

function Nature()
gg.setRanges(gg.REGION_C_ALLOC)
gg.searchNumber("24;1500;3600", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("3600", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
revert = gg.getResults(5000, nil, nil, nil, nil, nil, nil, nil, nil)
gg.editAll("0", gg.TYPE_DWORD)
gg.clearResults()
end

