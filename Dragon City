Start = 1

-------Phần Menu Choice------
function Start()
MenuScript = gg.choice({'Level 100.000','Farm 6 Food 0 Second','Watch ADS (Hatch/Breed)','Skip 15 Minute','Terra Cover Any','Show Code Dragon City','No Expand','Move Habital','Remove Time Building Habital','End Script'},Last,'Design by VietNamese Only #1')


if MenuScript == nil then Noselect() end
if MenuScript == 1 then Function1() end
if MenuScript == 2 then Function2() end
if MenuScript == 3 then Function3() end
if MenuScript == 4 then Function4() end
if MenuScript == 5 then Function5() end
if MenuScript == 6 then Function6() end
if MenuScript == 7 then Function7() end
if MenuScript == 8 then Function8() end
if MenuScript == 9 then Function9() end
if MenuScript == 10 then os.exit() end
MainMenu = -1
end


-------Phần Function-----
function Function1()
List = gg.multiChoice({'Level 70','Level 65','Level 60','Level 55','Level 50','Level 45','Level 40','Level 35','Level 30','Level 25','Level 20','Level 15','Level 10','Back to Menu'},nil,'Level Up')

if List == nil then else
if List[1] == true then Level70() end
if List[2] == true then Level65() end
if List[3] == true  then Level60() end
if List[4] == true then Level55() end
if List[5] == true then Level50() end
if List[6] == true then Level45() end
if List[7] ==true then Level40() end
if List[8] == true then Level35() end
if List[9] == true then Level30() end
if List[10] == true then Level25() end
if List[11] == true then Level20() end
if List[12] == true then Level15() end
if List[13] == true then Level10() end
if List [14] == true then Start() end
end end

function Level70()
gg.setRanges(gg.REGION_C_ALLOC)
gg.searchNumber("1011~2900;276::100", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("276", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
revert = gg.getResults(1000)
gg.editAll("399996", gg.TYPE_DWORD)
gg.clearResults()
end

